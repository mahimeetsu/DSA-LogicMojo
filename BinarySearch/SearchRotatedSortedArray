class Solution {
    public int search(int[] nums, int target) {
		int n = nums.length, low = 0, high = n - 1;
		while (low <= high) {
			int mid = (low + high) / 2;
			if (nums[mid] == target)
				return mid;
			if (nums[low] <= nums[mid]) {
				if (target >= nums[low] && target <= nums[mid]) {
					high = mid;
				} else {
					low = mid + 1;
				}
			} else if (nums[mid] < nums[high]) {
				if (target >= nums[mid] && target <= nums[high])
					low = mid + 1;
				else
					high = mid;
			}
		}
		return -1;
    }
}
