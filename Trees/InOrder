
class Node {
    int data;
    Node left, right;
   Node(int item)    {
        data = item;
        left = right = null;
    }
}
class Solution {
    ArrayList<Integer> inOrder(Node root) {
        ArrayList<Integer> list=new ArrayList<Integer>();
        helper(root,list);
        return list;
    }
    public void helper(Node root,ArrayList<Integer> list)
    {
        if(root==null)
            return;
        helper(root.left,list);
        list.add(root.data);
        helper(root.right,list);
    }
}
